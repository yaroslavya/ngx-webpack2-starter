{
  "name": "angular-webpack2-fast-build-starter",
  "version": "0.0.1-beta0",
  "description": "angular2+ webpack 2 boilerplate project that aims for the fast build and rebuild",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "webpack2",
    "webpack3",
    "karma",
    "chrome-headless",
    "performance",
    "typescript"
  ],
  "author": "Yaroslav Yakovlev <yaroslavya@gmail.com>",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "webpack-dev-server --config cfg/webpack.dev.ts --progress --profile",
    "build:dll": "webpack --config cfg/webpack.dll.ts --profile --progress",
    "postinstall": "npm run build:dll",
    "test:unit:dev": "karma start cfg/karma.dev.js",
    "test:unit:dev:coverage": "karma start cfg/karma.dev.js",
    "start": "npm run dev"
  },
